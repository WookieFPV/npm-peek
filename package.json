{
	"name": "npm-peek",
	"description": "Interactively compare npm packages to see what has changed",
	"license": "MIT",
	"type": "module",
	"version": "0.2.0",
	"files": [
		"dist",
		"template.html"
	],
	"bin": {
		"npm-peek": "dist/cli.js"
	},
	"keywords": [
		"npm",
		"diff",
		"viewer"
	],
	"main": "dist/cli.js",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/WookieFPV/npm-peek.git"
	},
	"homepage": "https://www.npmjs.com/package/npm-peek",
	"scripts": {
		"build": "tsup",
		"build:dev": "tsup --watch --clean false",
		"lint": "bunx biome check --fix",
		"lint:CI": "bunx biome check",
		"typecheck": "tsc",
		"prepublishOnly": "bun i && bun run lint:CI && tsc  && bun run build"
	},
	"dependencies": {
		"@inquirer/prompts": "7.10.1",
		"@rnx-kit/tools-workspaces": "0.2.3",
		"@stricli/core": "1.2.4",
		"diff2html": "3.4.52",
		"diff2html-cli": "5.2.15",
		"fast-fuzzy": "1.12.0",
		"open": "10.2.0",
		"ora": "9.0.0"
	},
	"devDependencies": {
		"@biomejs/biome": "2.3.8",
		"@types/bun": "1.3.4",
		"lefthook": "2.0.9",
		"tsup": "8.5.1",
		"typescript": "5.9.3"
	}
}
